<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_dynamic_tertiary60"
    tools:context=".ui.FinishFragment">

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="100dp"
        android:ems="10"
        android:inputType="text"
        android:text="BİLGİLER"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/resim" />

    <TextView
        android:id="@+id/email1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="E-mail:"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/sifre1"
        app:layout_constraintEnd_toStartOf="@+id/email2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/email2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/sifre2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText6"
        app:layout_constraintVertical_bias="0.958" />

    <TextView
        android:id="@+id/nereden2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/nereye2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tarih2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="104dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tarih1"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/sifre2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/telefon2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/nereye2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/tarih2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/telefon2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/nereden2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/sifre1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="Şifre:"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/telefon1"
        app:layout_constraintEnd_toStartOf="@+id/sifre2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/telefon1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="Telefon Numarası:"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/nereden1"
        app:layout_constraintEnd_toStartOf="@+id/telefon2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nereden1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="Nereden:"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/nereye1"
        app:layout_constraintEnd_toStartOf="@+id/nereden2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nereye1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="Nereye:"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/tarih1"
        app:layout_constraintEnd_toStartOf="@+id/nereye2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tarih1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="112dp"
        android:text="Yolculuk Tarihi:"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="32dp"
        android:text="Çıkış"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tarih2" />

    <TextView
        android:id="@+id/soyad1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="Kullanıcı Soyadı"
        app:layout_constraintBottom_toTopOf="@+id/email1"
        app:layout_constraintEnd_toStartOf="@+id/soyad2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/ad1"
        android:layout_width="110dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="10dp"
        android:text="Kullanıcı Adı"
        app:layout_constraintBottom_toTopOf="@+id/soyad1"
        app:layout_constraintEnd_toStartOf="@+id/soyad2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/ad2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/soyad2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/soyad2"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/email2"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>